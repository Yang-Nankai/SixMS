form {\r\n      display: flex;\r\n      flex-direction: column;\r\n      justify-content: center;\r\n      //align-items: center;\r\n\r\n      .input {\r\n        width: 100%;\r\n\r\n        & /deep/ .el-form-item__label {\r\n          line-height: 40px;\r\n          color: rgba(105, 105, 105, 1);\r\n          font-size: 16px;\r\n          font-weight: bold;\r\n          padding-right: 40px;\r\n        }\r\n\r\n        & /deep/ .el-input__inner {\r\n          height: 40px;\r\n          color: #606266;\r\n          font-size: 14px;\r\n          border-width: 0px;\r\n          border-style: solid;\r\n          border-color: #606266;\r\n          border-radius: 4px;\r\n          background-color: #fff;\r\n        }\r\n      }\r\n\r\n      .btn {\r\n        width: 200px;\r\n        height: 44px;\r\n        color: #fff;\r\n        font-size: 14px;\r\n        border-width: 0px;\r\n        border-style: solid;\r\n        border-color: #409EFF;\r\n        border-radius: 4px;\r\n        background-color: rgba(0, 206, 209, 0.64);\r\n        margin: 0 auto;\r\n      }\r\n    }\r\n  }\r\n}\r\n</style>\r\n"]}]}